<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.goodee.myJoinTree.mapper.LoginMapper">
	<select id="selectAccount" parameterType="com.goodee.myJoinTree.vo.AccountList" resultType="com.goodee.myJoinTree.vo.AccountList">
		SELECT emp_no empNo 
		FROM account_list
		<where>
			emp_no = #{empNo} AND emp_pw = password(#{empPw})
		</where>
	</select>
</mapper>