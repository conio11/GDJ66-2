package com.goodee.webdb.service;

import java.util.List;

import com.goodee.webdb.mapper.StudentMapper;
import com.goodee.webdb.vo.Student;

public class StudentService {
	private StudentMapper studentMapper; // new() 호출할 클래스 만들어지지 않음 -> 부모 타입 사용
	
	public List<Student> getStudentAll() {
		List<Student> list = studentMapper.selectStudentAll();  // 서비스 - 메소드에서 예외 발생 시 롤백
		return studentMapper.selectStudentAll();
	}
}
